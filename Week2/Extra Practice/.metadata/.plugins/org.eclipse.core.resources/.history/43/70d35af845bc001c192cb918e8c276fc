package zookeeperpart1;

public class Mammal {
	
	private int energyLvl;
	
	public Mammal(int energyLvl) {
		this.energyLvl = energyLvl;
	}

	public int getEnergyLvl() {
		return energyLvl;
	}

	public void setEnergyLvl(int energyLvl) {
		this.energyLvl = energyLvl;
	}
	
	public int displayEnergy() {
		System.out.printf("%s has an energy level of %s", this, this.energyLvl);
		return this.energyLvl;
	}
	
}


